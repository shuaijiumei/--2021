/* program/pages/logs/info_change/info_change.wxss */

.page{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;

}
.page>view{
  display: flex;
  flex-direction: column;
  justify-content: space-evenly;
  align-items: center;
  border-radius: 20px;
  height: 600rpx;
  width: 90%;

  background-color: #dcf6f0;
}

.page>view:nth-child(1)>view:nth-child(1){
  font-weight: 600;
  color: gray;
  font-size: medium;
}